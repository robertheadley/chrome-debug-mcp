{
  "manifest_version": 2,
  "name": "Test Extension",
  "version": "1.0",
  "description": "Simple test extension",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["test.js"],
      "run_at": "document_end"
    }
  ]
}